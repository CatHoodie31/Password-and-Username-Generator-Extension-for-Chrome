{
  "manifest_version": 3,
  "name": "Password & Username Generator",
  "version": "1.1",
  "description": "Generates password and username.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "web_accessible_resources": [
    {
      "resources": ["styles.css"],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": []
}
